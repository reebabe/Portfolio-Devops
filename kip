[32m+[m[32mFROM node:13.12.0-alpine[m
[32m+[m
[32m+[m[32m# set working directory[m
[32m+[m[32mWORKDIR /app[m
[32m+[m
[32m+[m[32m# add `/app/node_modules/.bin` to $PATH[m
[32m+[m[32mENV PATH /app/node_modules/.bin:$PATH[m
[32m+[m
[32m+[m[32m# install app dependencies[m
[32m+[m[32mCOPY package.json ./[m
[32m+[m[32mCOPY package-lock.json ./[m
[32m+[m[32mRUN npm install --silent[m
[32m+[m[32mRUN npm install[m[41m [m
[32m+[m[32m#react-scripts@3.4.1 -g --silent[m
[32m+[m
[32m+[m[32m# add app[m
[32m+[m[32mCOPY . ./[m
[32m+[m
[32m+[m[32m# start app[m
[32m+[m[32mCMD ["npm", "start"][m
